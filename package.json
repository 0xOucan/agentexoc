{
  "name": "agentkit-dapp",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "start": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "start:server": "cd server && npm start",
    "start:client": "cd client && npm start",
    "build": "concurrently \"npm run build:server\" \"npm run build:client\"",
    "build:server": "cd server && npm run build",
    "build:client": "cd client && npm run build",
    "clean": "concurrently \"cd server && npm run clean\" \"cd client && npm run clean\"",
    "dev": "concurrently \"cd server && npm run dev\" \"cd client && npm run start\"",
    "demo": "cd server && ts-node src/scripts/demo.ts"
  },
  "devDependencies": {
    "@types/dompurify": "^3.0.5",
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "@coinbase/agentkit": "^0.2.2",
    "@coinbase/agentkit-langchain": "^0.2.2",
    "dompurify": "^3.2.4"
  }
}
