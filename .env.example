# Copy this file to .env and fill in your own credentials.

# API Keys
OPENAI_API_KEY="your_openai_api_key_here"
CDP_API_KEY_NAME="your_cdp_api_key_name_here"
CDP_API_KEY_PRIVATE_KEY="your_cdp_private_key_here"

# Network Configuration
NETWORK_ID="base-mainnet"
NETWORK_ID_2="base-sepolia"

# Wallet Configuration (DO NOT SHARE REAL PRIVATE KEY!)
WALLET_PRIVATE_KEY="your_wallet_private_key_here"

# Telegram Bot (Optional)
TELEGRAM_BOT_TOKEN="your_telegram_bot_token_here"

# Server Configuration
PORT=3001
NODE_ENV=development

# Base Configuration
BASE_API_URL=https://api.base.org
BASE_EXPLORER_URL=https://basescan.org

# Security Settings
MAX_RETRIES=3
REQUEST_TIMEOUT=30000

# Feature Flags
ENABLE_TELEGRAM_BOT=false
ENABLE_AUTO_MODE=false
ENABLE_DEMO_MODE=true

# Note: Replace all placeholder values with your actual credentials
# Never commit the real .env file to version control

